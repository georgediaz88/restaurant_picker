class Restaurant extends Controller('restaurantPicker')
  constructor: ($http, $state, Restaurant) ->
    vm = @
    vm.restaurant = new Restaurant()

    vm.submit = ->
      vm.formPromise = vm.restaurant.create()
        .then ->
          $state.go('home')
        .catch ->
          console.log 'errored! Boo!'
          # Do something with error

